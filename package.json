{
  "name": "pixel",
  "version": "0.0.0-beta.1",
  "description": "Parse file to ImageData in cross-platform",

  "files": [
    "index.js",
    "lib"
  ],

  "scripts": {
    "default": "jasminetea test --file index.coffee --verbose --lint",

    "start": "$(opc package scripts.default) --watch",
    "localhost": "zuul test --ui jasmine2 --local 59798 --open",
    "build-dev": "$(opc bower opc-scripts.build) > $(opc bower name).js",

    "cover": "$(opc package scripts.default) --cover",
    "test": "npm run cover",
    "posttest": "zuul test && echo Perfect posttest || echo Ignored posttest",

    "prepublish": "node -e \"if(process.env.TRAVIS){}else{process.exit(1)}\" && npm run compile || echo skip prepublish",
    "compile": "coffee --bare --compile index.coffee",
    "postcompile": "coffee --output lib --bare --compile src",

    "build": "$(opc bower opc-scripts.build) | uglifyjs -cm > $(opc bower name).min.js",
    "postbuild": "wc -c $(opc bower main)",
    "update": "git tag v$(opc package version) && git push --tags"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover"
    }
  },

  "dependencies": {
    "pixel-bmp": "0.0.0-alpha.0",
    "pixel-gif": "0.0.0-alpha.0",
    "pixel-jpg": "0.0.0-alpha.0",
    "pixel-png": "0.0.0-alpha.0",
    "pixel-util": "0.0.0-beta.0"
  },
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^10.2.1",
    "coffee-script": "^1.9.2",
    "coffeeify": "^1.1.0",
    "fixture-images": "^0.1.4",
    "ghooks": "^0.3.2",
    "jasminetea": "^0.2.0-beta.1",
    "object-parser-cli": "0.0.0",
    "uglify-js": "^2.4.23",
    "zuul": "^3.0.0"
  },
  
  "keywords": [
    "pixel",
    "png",
    "jpg",
    "gif",
    "bmp"
  ],
  "author": "59naga <i59naga@icloud.com> (http://berabou.me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/59naga/pixel.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/pixel/issues"
  },
  "homepage": "https://github.com/59naga/pixel#readme"
}
